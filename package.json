  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/willrackley/Workr/issues"
  },
  "homepage": "https://github.com/willrackley/Workr#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "google-map-react": "^1.1.4",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.22",
    "nodemailer": "^6.1.0",
    "passport": "^0.4.0",
